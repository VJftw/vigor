{
  "go.enableCodeLens": {
    "runtest": false
  },
  "gopls": {
    "build.env": {
      "GOOS": "js",
      "GOARCH": "wasm"
    },
    "formatting.gofumpt": true,
    "ui.completion.usePlaceholders": true,
    "ui.semanticTokens": true,
    "ui.codelenses": {
      "gc_details": false,
      "regenerate_cgo": false,
      "generate": false,
      "test": true,
      "tidy": false,
      "upgrade_dependency": false,
      "vendor": false
    },
  },
  "go.useLanguageServer": true,
  "go.lintOnSave": "off",
  "go.vetOnSave": "off",
}
